#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/bin/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../uuid/3.3.2/node_modules/uuid/bin/uuid" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/bin/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/3.3.2/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/uuid/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../uuid/3.3.2/node_modules/uuid/bin/uuid" "$@"
  ret=$?
fi
exit $ret
