#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../needle/2.4.0/node_modules/needle/bin/needle" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/2.4.0/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/needle/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../needle/2.4.0/node_modules/needle/bin/needle" "$@"
  ret=$?
fi
exit $ret
