#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules/rimraf/bin.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules/rimraf/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../rimraf/2.6.3/node_modules/rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules/rimraf/bin.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules/rimraf/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/2.6.3/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rimraf/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../rimraf/2.6.3/node_modules/rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
