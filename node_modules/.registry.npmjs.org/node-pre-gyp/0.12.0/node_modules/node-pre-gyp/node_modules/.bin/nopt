#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/bin/nopt.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../nopt/4.0.1/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/bin/nopt.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules/nopt/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/4.0.1/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/nopt/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../nopt/4.0.1/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
