#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/cli.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../rc/1.2.8/node_modules/rc/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/cli.js/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules/rc/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/1.2.8/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/rc/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../rc/1.2.8/node_modules/rc/cli.js" "$@"
  ret=$?
fi
exit $ret
