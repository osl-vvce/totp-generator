#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
  ret=$?
else 
  NODE_PATH="/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/0.12.0/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node-pre-gyp/node_modules:/home/localhost/repository/totp-generator/server/node_modules/.registry.npmjs.org/node_modules:/home/localhost/repository/totp-generator/server/node_modules:/home/localhost/repository/totp-generator/node_modules:/home/localhost/repository/node_modules:/home/localhost/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../node-pre-gyp/0.12.0/node_modules/node-pre-gyp/bin/node-pre-gyp" "$@"
  ret=$?
fi
exit $ret
